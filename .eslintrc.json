{
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": [
      "standard-with-typescript",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  }
  